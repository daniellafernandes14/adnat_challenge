      <%= link_to 'Edit', edit_organisation_path(organisation) %>
      <%# <%= link_to 'Join',  %>
      <%# <%raise%>
    </li>
  <% end %>
</ul>

<h2>Create Organisation</h2>
<%= render 'shared/createform', organisation: @organisation %>
